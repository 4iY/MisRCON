{
  "name": "misrcon",
  "productName": "MisRCON",
  "version": "3.0.1-rc-6",
  "description": "RCON Tool and Map for Miscreated game.",
  "repository": {
    "url": "https://github.com/csprance/misrcon.git"
  },
  "main": "src/index.ts",
  "scripts": {
    "dev": "electron-forge start",
    "test": "jest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "repl": "ts-node src/repl.ts",
    "prettier-all": "prettier --write **/*.{ts,js,css,html} --single-quote true",
    "lint": "tslint --project tsconfig.json --force",
    "lint-fix": "tslint --project tsconfig.json --force --fix"
  },
  "keywords": [
    "miscreated",
    "rcon",
    "misrcon"
  ],
  "author": {
    "name": "Chris Sprance",
    "email": "chrissprance@gmail.com",
    "url": "https://github.com/csprance"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "asar": true,
        "packageManager": "yarn",
        "icon": "src/resources/icon",
        "ignore": [
          ".idea",
          "logs",
          ".github",
          ".cache"
        ]
      },
      "electronWinstallerConfig": {
        "name": "misrcon"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "csprance",
        "name": "misrcon"
      },
      "windowsStoreConfig": {
        "packageName": "MisRCON",
        "name": "misrcon"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.3",
    "@trodi/electron-splashscreen": "^0.3.4",
    "ag-grid-community": "^21.2.0",
    "ag-grid-react": "^21.2.0",
    "async-javascript-terminal": "^2.0.5",
    "axios": "^0.19.0",
    "cron": "^1.7.1",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "formik": "^2.0.1-rc.13",
    "get-options": "^1.2.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.5.1",
    "leaflet-mouse-position": "^1.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mousetrap": "^1.6.3",
    "node-misrcon": "^2.1.2",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-leaflet": "^2.4.0",
    "react-markdown": "^4.1.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "react-simple-code-editor": "^0.9.14",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "tslib": "^1.10.0",
    "typesafe-actions": "^5.0.0-3",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.2",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^24.0.17",
    "@types/leaflet": "^1.5.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.7.1",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.9.2",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.9.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-leaflet": "^2.4.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-router-redux": "^5.0.18",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^4.1.18",
    "@types/yup": "^0.26.23",
    "electron": "4.0.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "typescript": "latest"
  }
}

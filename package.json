  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.0",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.31",
    "@types/react-router-dom": "^4.3.1",

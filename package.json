{
  "name": "misrcon",
  "productName": "MisRCON",
  "version": "3.0.1",
  "description": "RCON Tool for Miscreated game.",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start -l",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force"
  },
  "keywords": [
    "miscreated",
    "rcon",
    "misrcon"
  ],
  "author": {
    "name": "Chris Sprance",
    "email": "chrissprance@gmail.com",
    "url": "https://github.com/csprance"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "misrcon"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "csprance",
        "name": "misrcon"
      },
      "windowsStoreConfig": {
        "packageName": "MisRCON",
        "name": "misrcon"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "@trodi/electron-splashscreen": "^0.3.1",
    "async-javascript-terminal": "^2.0.1",
    "axios": "^0.18.0",
    "electron": "3.0.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "font-awesome": "^4.7.0",
    "get-options": "^1.1.1",
    "history": "^4.7.2",
    "node-cron": "^2.0.1",
    "node-misrcon": "^1.0.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",

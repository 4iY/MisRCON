{
  "name": "misrcon",
  "productName": "MisRCON",
  "version": "3.0.0-dev-16",
  "description": "RCON Tool and Map for Miscreated game.",
  "main": ".webpack/main",
  "scripts": {
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "repl": "ts-node src/repl.ts",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix"
  },
  "repository": {
    "url": "https://github.com/csprance/misrcon.git"
  },
  "keywords": [
    "miscreated",
    "rcon",
    "misrcon"
  ],
  "author": {
    "name": "Chris Sprance",
    "email": "chrissprance@gmail.com",
    "url": "https://github.com/csprance"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "csprance",
              "name": "misrcon"
            },
            "prerelease": true
          }
        }
      ],
      "electronRebuildConfig": {

      },
      "packagerConfig": {
        "prune": true,
        "asar": true,
        "packageManager": "yarn",
        "icon": "public/icon",
        "extraResource": [
          "resources/maps"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "misrcon"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "@trodi/electron-splashscreen": "^0.3.4",
    "ag-grid-community": "^21.1.1",
    "ag-grid-react": "^21.1.0",
    "async-javascript-terminal": "^2.0.5",
    "axios": "^0.19.0",
    "brace": "^0.11.1",
    "cron": "^1.7.1",
    "date-fns": "^1.30.1",
    "electron-squirrel-startup": "^1.0.0",
    "get-options": "^1.2.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.5.1",
    "leaflet-mouse-position": "^1.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mousetrap": "^1.6.3",
    "node-misrcon": "^2.1.2",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-ace": "^7.0.2",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11",
    "react-leaflet": "^2.4.0",
    "react-markdown": "^4.1.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "react-simple-code-editor": "^0.9.14",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "reselect": "^4.0.0",
    "sqlite3": "^4.0.9",
    "styled-components": "^4.3.2",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.18",
    "typesafe-actions": "^4.4.2",
    "update-electron-app": "^1.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.39",
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "@electron-forge/maker-rpm": "^6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
    "@electron-forge/maker-zip": "^6.0.0-beta.39",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.39",
    "@electron-forge/publisher-github": "^6.0.0-beta.39",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/cron": "^1.7.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.2",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^24.0.17",
    "@types/leaflet": "^1.5.0",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.7.1",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.9.1",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.8.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-leaflet": "^2.2.1",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-router-redux": "^5.0.18",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "css-loader": "^3.2.0",
    "electron": "6.0.2",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.12.11",
    "style-loader": "^1.0.0",
    "tslint": "^5.18.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2"
  }
}
